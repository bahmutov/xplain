{
  "name": "xplain",
  "version": "0.0.0",
  "description": "Generates API documentation with unit tested examples",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "test": "gt test/*.js",
    "yuidoc": "yuidoc src test",
    "doc": "gt test/*.js && node index.js -i src/*.js,test/*.js -v 1.0.1",
    "complexity": "jsc -p src/*.js",
    "example1": "node index.js -i examples/basic/src/*.js,examples/basic/test/*.js -o examples/basic/docs",
    "example2": "node index.js -i examples/submodules/*.js -o examples/submodules/docs"
  },
  "repository": "",
  "keywords": [
    "javascript",
    "api",
    "doc",
    "node"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "dox": "~0.4.3",
    "check-types": "~0.5.2",
    "esprima": "~1.0.2",
    "escodegen": "0.0.20",
    "lodash": "~1.1.1",
    "js-beautify": "~1.3.1",
    "moment": "~2.0.0",
    "commander": "~1.1.1",
    "glob": "~3.2.1",
    "allong.es": "~0.13.0",
    "pithy": "0.0.3",
    "html": "0.0.7",
    "mkdirp": "~0.3.5",
    "fs.extra": "~1.2.0"
  },
  "devDependencies": {
    "gt": ">0.8.2"
  }
}